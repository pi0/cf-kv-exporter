{
  "name": "cf-kv-exporter",
  "version": "0.0.1",
  "description": "CLI to export Cloudflare KV Namespace",
  "repository": "pi0/cf-kv-exporter",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "bin": {
    "cf-kv-exporter": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint --cache . && prettier -c src",
    "lint:fix": "eslint --cache . --fix && prettier -c src -w",
    "prepack": "pnpm run build",
    "cf-kv-exporter": "jiti ./src/cli.ts",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "cli-progress": "^3.12.0",
    "consola": "^3.4.2",
    "unstorage": "^1.17.3"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^24.10.4",
    "@upstash/redis": "^1.35.8",
    "changelogen": "^0.6.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-unjs": "^0.5.0",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1"
  },
  "packageManager": "pnpm@10.26.1"
}