{
  "name": "cf-kv-exporter",
  "version": "0.0.1",
  "description": "CLI to export Cloudflare KV Namespace",
  "repository": "pi0/cf-kv-exporter",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "bin": {
    "cf-kv-exporter": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src -w",
    "prepack": "pnpm run build",
    "cf-kv-exporter": "jiti ./src/cli.ts",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "cli-progress": "^3.12.0",
    "consola": "^3.2.3",
    "unstorage": "^1.10.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/node": "^20.12.5",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "eslint-config-unjs": "^0.2.1",
    "jiti": "^1.21.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.4",
    "unbuild": "^2.0.0"
  },
  "packageManager": "pnpm@10.13.1"
}